<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recording-proxy-config>
<proxy-port>8888</proxy-port>
<output-file>outputFile.xml</output-file>
<inclusions/>
<exclusions/>
<body-inclusions>
</body-inclusions>
<body-exclusions>
<rule check="response" header="content-type" match="contains">image</rule>
<rule check="response" header="content-type" match="contains">pdf</rule>
<rule check="response" header="content-type" match="contains">mp3</rule>
<rule check="response" header="content-type" match="contains">mp4</rule>
<rule check="response" header="content-type" match="contains">shockwave</rule>
</body-exclusions>
</recording-proxy-config>
